#include <stdio.h>
#define MAXN 10
int get_max(int n, int l[]) {
    int i,j,k;
    int sum = 0, max_sum = 0;
    for (i=0; i<n; i++) {              //左头
        for(j=i;j<n;j++)    {          //右头
            sum = 0;                    //本次计数重置
        for(k=i;k<j;k++)   {
            sum += l[j];                //本次累加
        if (sum >= max_sum) {
            max_sum = sum;              //比较结果
            }
        }
    }
    return(max_sum);
    }
}

int main()
{
    int i, n;
    int l[MAXN];

    scanf("%d", &n);
    for( i=0; i<n; i++ )
        scanf("%d", &l[i]);
    printf("%d\n",get_max(n, l));

    return 0;
}
